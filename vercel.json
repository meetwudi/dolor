{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "bunVersion": "1.x",
  "functions": {
    "api/queue.ts": {
      "experimentalTriggers": [
        {
          "type": "queue/v1beta",
          "topic": "telegram-updates-v2",
          "consumer": "telegram-webhook"
        }
      ]
    }
  },
  "rewrites": [
    {
      "source": "/api/telegram",
      "destination": "/api/telegram"
    },
    {
      "source": "/api/web/:path*",
      "destination": "/api/web?path=:path*"
    },
    {
      "source": "/auth/web/login",
      "destination": "/api/web?path=auth/login"
    },
    {
      "source": "/auth/web/callback",
      "destination": "/api/web?path=auth/callback"
    },
    {
      "source": "/connect",
      "destination": "/api/connect"
    },
    {
      "source": "/auth/intervals/callback",
      "destination": "/api/intervals-callback"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
